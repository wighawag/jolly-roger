    "svelte": "^3.43.2",
    "svelte-preprocess": "^4.9.8",
    "sveltejs-adapter-ipfs": "0.1.6",
    "tailwindcss": "^2.2.16",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vite": "2.6.7"
  },
  "scripts": {
    "prepare": "node ./.setup.cjs && prepare-for-web --target static --targetFile src/app.html",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "serve": "ipfs-emulator --only -d build -p 8080",
    "deploy": "dotenv -- ipfs-deploy -p pinata -s jolly-roger build",
    "build:deploy": "run-s build deploy"
