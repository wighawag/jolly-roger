  "devDependencies": {
    "@sveltejs/adapter-node": "1.0.0-next.54",
    "@sveltejs/adapter-static": "1.0.0-next.21",
    "@sveltejs/kit": "1.0.0-next.182",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
