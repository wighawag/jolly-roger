{
  "name": "jolly-roger-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 12.17.0"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.4.1",
    "@ethersproject/abstract-provider": "^5.4.1",
